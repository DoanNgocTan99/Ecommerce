                <div class="row">
                    <div class="col-lg-5 col-md-12 col-12">
                        <!-- Top Left -->
                        <div class="top-left">
                            <ul class="list-main">
                                <li><i class="fa fa-home"></i> <a href="#">Kênh người bán</a></li>
                                </li>
                                <li><i class="fa fa-envelope-square"></i> <a href="https://www.google.com/intl/vi/gmail/about/#" target="_blank">electronicshop@gmail.com</a></li>
                            </ul>
                        </div>
                        <!--/ End Top Left -->
                    </div>
                    <div class="col-lg-7 col-md-12 col-12">
                        <!-- Top Right -->
                        <div class="right-content">
                            @{ if (Session["Name"] != null)
                                {
                                    <ul class="list-main">
                                        <li><i class="fa fa-user-circle"></i> <a href="/user/edit/@Session["IdUser"]">Hi! @Session["Name"]</a></li>
                                        <li><i class="fa fa-sign-in-alt"></i><a href="/Login/Logout">Đăng xuất</a></li>
                                    </ul>
                                }
                                else
                                {

                                    <ul class="list-main">
                                        <li><i class="fa fa-power-off"></i><a href="/Login/Index">Đăng nhập</a></li>
                                        <li><i class="fas fa-user-plus"></i><a href="/Login/register">Đăng Ký</a></li>
                                    </ul>

                                }
                            }
                        </div>
                        <!-- End Top Right -->
                    </div>
                </div>
            </div>
        </div>
        <!-- End Topbar -->
        <div class="middle-inner">
            <div class="container">
                <div class="row">
                    <!-- Logo -->
                    <div class="logo">
                        <a href="/"><img src="~/Assets/User/images/logo-login-small.png" alt="logo img" width="100%"></a>
                    </div>
                    <!--/ End Logo -->
                    <!-- Search Form -->
                    <div class="search-bar-box">
                        <form action="/products/search" method="GET" class="form-search">
                            <input class="search_input" id="search-keyword" name="key" type="search" placeholder="Tìm kiếm sản phẩm...">
                            <button href="/products/search" class="search_btn" type="submit"><span class="fas fa-search"></span></button>
                        </form>
                    </div>
                    <div class="right-bar">
                        <!-- Search Form -->

                        <div class="sinlge-bar shopping">
                            <a href="/cart/cart" class="single-icon">
                                <span class="fas fa-cart-arrow-down"></span>
                                <span class="aa-cart-title">GIỎ HÀNG</span>
                                <span class="total-count">@Html.Action("cart_partial", "cart")</span>
                            </a>
                            <!-- Shopping Item -->
                            @*<div class="shopping-item">
                                    <div class="dropdown-cart-header">
                                        <span>@Html.Action("cart_partial", "cart")  Sản phẩm</span>
                                        <a href="/cart/cart">Xem giỏ hàng</a>
                                    </div>
                                    @Html.Action("LoadCart","Cart")
                                </div>*@
                            <!--/ End Shopping Item -->
                        </div>
                    </div>

                </div>
            </div>
